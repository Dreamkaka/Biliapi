{
    "version": 2,
    "regions": ["sfo1"],
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH"
        ]
      }
    ],
    {
  "regions": ["sfo1"]
   },
  }
